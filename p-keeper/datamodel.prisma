
type Experience {
  id: ID! @id
  location: String! 
  state: String! 
  destination: String!
  time: String
  date: String!
  description: String!
  robbed: Boolean!
  items: String
  car: Car! @relation(name:"CarToExperience", onDelete: CASCADE)
}

type Car {
  id: ID! @id
  color: String
  plateNumber: String
  passengers: Int
  make: String
  description: String
  experience: Experience @relation(link:INLINE, name:"CarToExperience")
}

type Tip {
  id: ID! @id
  yourTip: String
  twitterHandle: String
}