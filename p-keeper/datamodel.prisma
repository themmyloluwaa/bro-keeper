type Experience {
  id: ID! @id
  location: String! @unique
  state: String! @unique
  destination: String!
  time: String
  date: String!
  description: String!
  robbed: Boolean!
  items: String
  car: Car! @relation(name:"CarToExperience", onDelete: CASCADE)
}

type Car {
  id: ID! @id
  color: String
  plateNumber: String
  passangers: Int
  make: String
  description: String
}
